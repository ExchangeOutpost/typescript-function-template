{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "lib": [],
    "types": ["@extism/js-pdk", "node"],
    "outDir": "./dist",
    "rootDir": "./src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": false
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules", "dist"]
}
